    "kind" : "Deployment",
    "metadata" : {
      "annotations" : {
        "fabric8.io/metrics-path" : "dashboard/file/kubernetes-pods.json/?var-project=helloWorld&var-version=1.0.0-SNAPSHOT"
      },
      "labels" : {
        "app" : "helloWorld",
        "provider" : "fabric8",
        "version" : "1.0.0-SNAPSHOT",
        "group" : "com.tibco.bw"
      },
      "name" : "bwce-sample"
    },
    "spec" : {
      "replicas" : 1,
      "revisionHistoryLimit" : 2,
      "selector" : {
        "matchLabels" : {
          "app" : "helloWorld",
          "provider" : "fabric8",
          "group" : "com.tibco.bw"
        }
      },
      "template" : {
        "metadata" : {
          "annotations" : {
            "fabric8.io/metrics-path" : "dashboard/file/kubernetes-pods.json/?var-project=helloWorld&var-version=1.0.0-SNAPSHOT"
          },
          "labels" : {
            "container" : "bwce-sample",
            "project" : "bwce-sample",
            "app" : "helloWorld",
            "provider" : "fabric8",
            "version" : "1.0.0-SNAPSHOT",
            "group" : "com.tibco.bw"
          },
          "name" : "bwce-sample",
          "namespace" : "default"
        },
        "spec" : {
          "containers" : [ {
            "env" : [ {
              "name" : "APP_CONFIG_PROFILE",
              "value" : "docker"
