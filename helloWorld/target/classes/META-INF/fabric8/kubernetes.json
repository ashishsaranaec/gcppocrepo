    "metadata" : {
      "labels" : {
        "container" : "bwce-sample",
        "provider" : "fabric8",
        "project" : "bwce-sample",
        "group" : "com.tibco.bw",
        "expose" : "true",
        "app" : "helloWorld",
        "version" : "1.0.0-SNAPSHOT"
      },
      "name" : "bwce-sample-service",
      "namespace" : "default"
    },
    "spec" : {
      "ports" : [ {
        "port" : 80,
        "protocol" : "TCP",
        "targetPort" : 8080
      } ],
      "selector" : {
        "container" : "bwce-sample",
        "provider" : "fabric8",
        "project" : "bwce-sample",
        "group" : "com.tibco.bw",
        "app" : "helloWorld"
      },
      "type" : "LoadBalancer"
    }
  }, {
