            } ],
            "image" : "gcr.io/gcpbwcepoc/hello-world-rest-api:v1",
            "imagePullPolicy" : "Always",
            "name" : "bwce-sample",
            "ports" : [ {
              "containerPort" : 8080,
              "name" : "http"
